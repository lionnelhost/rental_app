<div class="header bg-primary pb-6">
  <div class="container-fluid">
    <div class="header-body">
      <%= render partial: 'shared/header', locals: {
            icon: 'home',
            breadcrumb_1: 'Dashboards',
            breadcrumb_2: 'Default',
            main_url: dashboard_path,
            breadcrumb_url: '#'
      } %>
      <%= render 'dashboard/metrics_summary' %>
    </div>
  </div>
</div>
<div class="container-fluid mt--6">
  <div class="row">
    <div class="col-xl-8">
      <%= render 'dashboard/charts/paiements' %>
    </div>
    <div class="col-xl-4">
      <%= render 'dashboard/charts/tenants' %>
    </div>
  </div>
  <div class="row">
    <div class="col-xl-8">
      <%#= render 'dashboard/tables/page_visits' %>
    </div>
    <div class="col-xl-4">
      <%#= render 'dashboard/tables/social_traffic' %>
    </div>
  </div>
  <%= render 'shared/footer' %>
</div>